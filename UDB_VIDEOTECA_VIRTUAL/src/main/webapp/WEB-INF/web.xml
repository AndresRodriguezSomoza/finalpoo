<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LibroControlador</servlet-name>
        <servlet-class>controlador.LibroControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LibroControlador</servlet-name>
        <url-pattern>/LibroControlador</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CdControlador</servlet-name>
        <servlet-class>controlador.CdControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CdControlador</servlet-name>
        <url-pattern>/CdControlador</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ObraControlador</servlet-name>
        <servlet-class>controlador.ObraControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ObraControlador</servlet-name>
        <url-pattern>/ObraControlador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
